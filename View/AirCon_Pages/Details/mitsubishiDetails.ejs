    // Function to update the AC details based on selected model
    function updateACDetails(modelId) {
      const modelData = acData[modelId];
      if (!modelData) return;

      // Update description
      document.getElementById('acDescription').textContent = modelData.description;

      // Update image
      document.getElementById('acImage').src = modelData.image;
      document.getElementById('acImage').alt = modelId;

      // Update characteristics
      const characteristicsList = document.getElementById('acCharacteristics');
      characteristicsList.innerHTML = '';
      modelData.characteristics.forEach(item => {
        const li = document.createElement('li');
        li.textContent = item;
        characteristicsList.appendChild(li);
      });

      // Update price
      document.getElementById('acPrice').textContent = modelData.price;

      // Update technical specifications table
      document.getElementById('coolingPower').textContent = modelData.technicalSpecs.coolingPower;
      document.getElementById('heatingPower').textContent = modelData.technicalSpecs.heatingPower;
      document.getElementById('noiseLevel').textContent = modelData.technicalSpecs.noiseLevelIndoor;
      document.getElementById('operatingTemp').textContent = modelData.technicalSpecs.operatingTemp;
      document.getElementById('dimensions').textContent = modelData.technicalSpecs.dimensionsIndoor;
      document.getElementById('warranty').textContent = modelData.technicalSpecs.warranty;
    }

    // Add event listener for model selection
    document.getElementById('acModelSelect').addEventListener('change', function() {
      const selectedModel = this.value;
      updateACDetails(selectedModel);
    });

    // Initialize with the default selected model
    document.addEventListener('DOMContentLoaded', function() {
      const defaultModel = document.getElementById('acModelSelect').value;
      updateACDetails(defaultModel);
    });

    // Scroll to top function
